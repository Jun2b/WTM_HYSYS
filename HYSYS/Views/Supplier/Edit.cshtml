@model HYSYS.ViewModels.SupplierVMs.SupplierVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.SupplierCode" />
<wt:textbox field="Entity.SupplierName" />
<wt:textbox field="Entity.PhoneNumber" />
<wt:textbox field="Entity.Remark" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>